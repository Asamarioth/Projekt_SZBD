@model IEnumerable<Projekt_SZBD.Models.KLIENT>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nazwa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Czy_Firma)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NIP)
        </th>
        <th>
            Adres
        </th>
        <th>
            Kontakt
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nazwa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Czy_Firma)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NIP)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ADRES.Ulica)   @Html.DisplayFor(modelItem => item.ADRES.Numer_Domu),  @Html.DisplayFor(modelItem => item.ADRES.Kod_Pocztowy)  @Html.DisplayFor(modelItem => item.ADRES.Miejscowosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KONTAKT.Telefon_1) @Html.DisplayFor(modelItem => item.KONTAKT.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_Klienta }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_Klienta }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_Klienta })
        </td>
    </tr>
}

</table>
